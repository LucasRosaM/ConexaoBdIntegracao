{
  "name": "api-test",
  "version": "1.0.0",
  "description": "Template",
  "main": "index.js",
  "scripts": {
    "cypress:db" : "node ./cypress/requests/connection.service.js && cypress open",
    "connect:db" : "node ./cypress/requests/connection.service.js", 
    "cypress:open": "cypress open",
    "cucumber": "cypress-tags run -e",
    "test": "cypress run --spec \"**/*.feature\"",
    "cypress:run": "cypress run",
    "precypress:run": "node ./cypress/support/clear.js",
    "cypress:report": "node ./cypress/support/report.js"
  },
  "dependencies": {
    "cypress-plugin-tab": "^1.0.5",
    "cypress-xpath": "^1.4.0",
    "fs": "0.0.1-security",
    "fs-extra": "^8.1.0",
    "gcc": "^1.0.2",
    "jsonschema": "^1.4.0",
    "multiple-cucumber-html-reporter": "^1.18.0",
    "oracledb": "^5.1.0",
    "rimraf": "^3.0.2"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": false,
    "step_definitions": "cypress/integration/step_definitions/",
    "cucumberJson": {
      "generate": true,
      "outputFolder": "cypress/cucumber-json",
      "filePrefix": "",
      "fileSuffix": ".cucumber"
    }
  },
  "devDependencies": {
    "cypress": "^7.0.0",
    "cypress-config-switcher": "^1.0.0",
    "cypress-cucumber-preprocessor": "^2.5.5"
  },
  "author": "Leonardo Kartabil",
  "license": "ISC"
}
